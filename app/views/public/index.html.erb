

<!DOCTYPE html>
<html>
<head>
  <title>Wilfred</title>
  <meta name="viewport" content="width=device-width" /> 
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="row"> 
  <header>
    <div class="container">

        <div class="three columns logo">
          <%= link_to root_path do %>
            <h4>Wilfred.</h4>
          <% end %>
        </div>

        <div class="nine columns">
          <ul class="navigation"> 
            <li><%= link_to "Learn More", "/about" %></li>
          </ul>
      </div>
    </div>
   </header>
  </div>

  <div class="row"> 
  <div class="container content"> 
    <div class="twelve columns"> 
      
      <div class="homepage-content">
        <h1>Verify your commits.</h2>
        <%= link_to "Log in with Github",user_omniauth_authorize_path(:github), class: "btn-auth btn-github large" %>
       </div>
    </div>
  </div>
</div>
 </div>
</body>
</html>
